
<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Hello Admin</h3>
  </div>

<div class="container" >
  <% @users.each do |user| %>
    <li>
      <%= user.full_name %> - <%= user.email %>
    </li>
    <%= button_to "Promote", promote_path(user.id), method: :put %>
    <%= button_to "Demote", demote_path(user.id), method: :put %>
    <%= user.student %>
  <% end %>
</div>
